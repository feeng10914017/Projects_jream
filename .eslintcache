 './GoogleAPI'\r\nimport { Button } from 'react-bootstrap'\r\nimport './Location.css'\r\nimport { Row, Col, Card, Form } from 'react-bootstrap'\r\nimport { Locationdata } from './data/locationdata.js'\r\n\r\nfunction Location() {\r\n  const [item, setItem] = useState({\r\n    id: 0,\r\n    name: '',\r\n    lat: 23.797296,\r\n    lng: 121.03121,\r\n    address: '',\r\n    opentime: '',\r\n  })\r\n  const [dataFromLeft, setDataFromLeft] = useState(true)\r\n  // searching part--------------------------\r\n  const [searchQuery, setSearchQuery] = useState('')\r\n  const [doSearch, setDoSearch] = useState(false)\r\n  const [locations, setLocations] = useState(null)\r\n  useEffect(() => {\r\n    setLocations(\r\n      Locationdata.filter((location, index) => {\r\n        return (\r\n          location.name.includes(searchQuery) ||\r\n          location.address.includes(searchQuery) ||\r\n          location.phone.includes(searchQuery) ||\r\n          location.opentime.includes(searchQuery)\r\n        )\r\n      }).map((location, index) => {\r\n        return (\r\n          <li key={index}>\r\n            <div className=\"Location_item\">\r\n              <div className=\"photoBox\">\r\n                <img src={location.img} alt=\"Product\" />\r\n              </div>\r\n              <h5>{location.name}</h5>\r\n              <div className=\"address fontSize14\">門市地址</div>\r\n              <div className=\"time fontSize14\">營業時間</div>\r\n              <div className=\"phone fontSize14\">連絡電話</div>\r\n              <div className=\"addressA fontSize14\">{location.address}</div>\r\n              <div className=\"timeA fontSize14\">{location.opentime}</div>\r\n              <div className=\"phoneA fontSize14\">{location.phone}</div>\r\n\r\n              <Button\r\n                variant=\"primary\"\r\n                onClick={() => {\r\n                  setItem(location)\r\n                  setDataFromLeft(true)\r\n                }}\r\n              >\r\n                詳細資訊\r\n              </Button>\r\n            </div>\r\n          </li>\r\n        )\r\n      })\r\n    )\r\n  }, [doSearch])\r\n\r\n  return (\r\n    <div className=\"d-flex\">\r\n      <div\r\n        className=\"width_1 nopadding border flexSrink_0\"\r\n        style={{ height: 'calc(100vh - 80px)' }}\r\n      >\r\n        <div className=\"SearchingBox border\">\r\n          <Row>\r\n            <ul className=\"SearchingPlace\">\r\n              <Form.Control\r\n                className=\"searchingInp border \"\r\n                type=\"text\"\r\n                placeholder=\"據點搜尋\"\r\n                // ---------searching part--------------------------\r\n                value={searchQuery}\r\n                onKeyDown={(event) => {\r\n                  if (event.keyCode === 13) {\r\n                    setDoSearch(!doSearch)\r\n                  }\r\n                }}\r\n                onChange={(e) => setSearchQuery(e.target.value)}\r\n              />\r\n              {/* e is event， target : element， */}\r\n\r\n              {/* <Form.Control\r\n                className=\"searchingInp border \"\r\n                as=\"select\"\r\n                defaultValue=\"Choose\"\r\n              >\r\n                {Locationdata.map((location, index) => {\r\n                  return <option>{location.city}</option>\r\n                })}\r\n              </Form.Control> */}\r\n            </ul>\r\n\r\n            <Button\r\n              size=\"lg\"\r\n              variant=\"primary\"\r\n              className=\"SearchingBtn \"\r\n              onClick={() => {\r\n                setDoSearch(!doSearch)\r\n              }}\r\n            >\r\n              Search\r\n            </Button>\r\n          </Row>\r\n        </div>\r\n        <nav className=\"L_nav-menu \">\r\n          <ul className=\"nopadding\">{locations}</ul>\r\n        </nav>\r\n      </div>\r\n\r\n      <div\r\n        className=\"width_2 nopadding\"\r\n        style={{ height: 'calc(100vh - 80px)' }}\r\n      >\r\n        <Google\r\n          currentLocation={{ lat: item.lat, lng: item.lng }}\r\n          id={item.id}\r\n          name={item.name}\r\n          address={item.address}\r\n          opentime={item.opentime}\r\n          phone={item.phone}\r\n          img={item.img}\r\n          dataFromLeft={dataFromLeft}\r\n          setDataFromLeft={setDataFromLeft}\r\n        />\r\n      </div>\r\n    </div>\r\n  )\r\n}\r\n\r\nexport default Location\r\n","C:\\Users\\kevino430\\Desktop\\Project1_1\\project_JREAM\\src\\pages\\order\\OD_Checkout.js",["285","286"],"import React, { useState, useEffect } from 'react'\r\nimport { Row, Card, Button, Form } from 'react-bootstrap'\r\nimport { Link } from 'react-router-dom'\r\n\r\nimport './OD.scss'\r\nimport OrderStep from './OD_Ccomponents/OD_Step'\r\nimport GrandTotal from './OD_Ccomponents/OD_GrandTotal'\r\n\r\nfunction OD_Checkout() {\r\n  const [checkType, setCheckType] = useState('')\r\n  return (\r\n    <>\r\n      <article className=\"col-10\">\r\n        <OrderStep step=\"2\" />\r\n        <Card>\r\n          <Card.Header>\r\n            <h6>取貨和付款方式</h6>\r\n          </Card.Header>\r\n\r\n          <Card.Body className=\"invoiceChose\">\r\n            <Form>\r\n              <Form.Group as={Row} reqire>\r\n                <Form.Label></Form.Label>\r\n\r\n                <Row>\r\n                  <Form.Check\r\n                    type=\"radio\"\r\n                    label=\"7-11 取貨付款\"\r\n                    name=\"invoice\"\r\n                    id=\"seven\"\r\n                  />\r\n                  <p>\r\n                    <span>滿1000元免運費，運費 $0</span>\r\n                  </p>\r\n                </Row>\r\n                <div className=\"line\"></div>\r\n                <Row>\r\n                  <Form.Check\r\n                    type=\"radio\"\r\n                    label=\"全家 取貨付款\"\r\n                    name=\"invoice\"\r\n                    id=\"family\"\r\n                  />\r\n        