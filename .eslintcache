y')\r\n        .parent()\r\n        .next()\r\n        .addClass('activeColorSecondary')\r\n    props.step == 3 &&\r\n      $('#step3')\r\n        .children()\r\n        .eq(0)\r\n        .addClass('activBorder')\r\n        .children()\r\n        .addClass('activeColorPrimary')\r\n        .parent()\r\n        .next()\r\n        .addClass('activeColorSecondary')\r\n    props.step == 4 &&\r\n      $('#step4')\r\n        .children()\r\n        .eq(0)\r\n        .addClass('activBorder')\r\n        .children()\r\n        .addClass('activeColorPrimary')\r\n        .parent()\r\n        .next()\r\n        .addClass('activeColorSecondary')\r\n  }, [])\r\n  return (\r\n    <>\r\n      <div className=\"step\">\r\n        <div id=\"step1\">\r\n          <div>\r\n            <BiSearchAlt size=\"32\" />\r\n          </div>\r\n          <p>1. 確認訂單</p>\r\n        </div>\r\n        <AiOutlineLine size=\"48\" />\r\n        <div id=\"step2\">\r\n          <div>\r\n            <RiCoinsLine className=\"coin\" size=\"16\" />\r\n            <AiOutlineLine className=\"divide\" size=\"40\" />\r\n            <FiTruck className=\"truck\" size=\"16\" />\r\n          </div>\r\n          <p>2. 付款與運送</p>\r\n        </div>\r\n        <AiOutlineLine size=\"48\" />\r\n        <div id=\"step3\">\r\n          <div>\r\n            <AiOutlineForm size=\"32\" />\r\n          </div>\r\n          <p>3. 填寫資料</p>\r\n        </div>\r\n        <AiOutlineLine size=\"48\" />\r\n        <div id=\"step4\">\r\n          <div>\r\n            <AiOutlineFlag size=\"32\" />\r\n          </div>\r\n          <p>4. 完成訂單</p>\r\n        </div>\r\n      </div>\r\n    </>\r\n  )\r\n}\r\n\r\nexport default OD_Step\r\n","C:\\Users\\kevino430\\Desktop\\Project1_1\\project_JREAM\\src\\pages\\order\\OD_Ccomponents\\OD_CartMotor.js",["359"],"import React from 'react'\r\nimport { Row, Form, Card } from 'react-bootstrap'\r\n\r\nfunction OD_CartMotor(props) {\r\n  const MotortData = props.data\r\n  // console.log(MotortData)\r\n\r\n  